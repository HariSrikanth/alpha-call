OPENAI_API_KEY=your_openai_api_key_here
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
PHONE_NUMBER_FROM=your_twilio_phone_number_here
DOMAIN=your_google_cloud_domain_here
PORT=8080

# Phone number authorization (optional)
VERIFIED_PHONE_NUMBERS=+1234567890,+1987654321
ALLOW_ALL_US_CANADA=false

# Concurrency settings (optional)
MAX_CONCURRENT_CALLS=10
MAX_OPENAI_CONNECTIONS=20

# Database configuration
# Option 1: Google Cloud SQL connection name (recommended for production)
GOOGLE_CLOUD_SQL_CONNECTION_NAME=your-project:region:instance-name

# Option 2: Full database URL (alternative to individual parameters)
# DATABASE_URL=postgresql+asyncpg://user:password@host:port/database

# Database credentials
DB_USER=postgres
DB_PASSWORD=your_database_password
DB_NAME=voice_assistant

# Database debugging (optional)
DB_ECHO=false 